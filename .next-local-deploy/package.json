{
  "name": "saraiva-vision-site",
  "type": "module",
  "version": "2.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:vite": "vite",
    "build": "next build",
    "build:vite": "vite build && node scripts/prerender-pages.js",
    "build:norender": "vite build",
    "build:blog": "node scripts/build-blog-posts.js",
    "prebuild": "node scripts/build-blog-posts.js",
    "preview": "vite preview",
    "start": "next start -p 3000",
    "start:vite": "vite",
    "test": "jest",
    "test:jest": "jest",
    "test:jest:watch": "jest --watch",
    "test:jest:coverage": "jest --coverage",
    "test:vitest": "vitest",
    "test:vitest:run": "vitest run",
    "test:vitest:ui": "vitest --ui",
    "test:vitest:coverage": "vitest run --coverage",
    "test:comprehensive": "npm run test:unit && npm run test:integration && npm run test:a11y && npm run test:e2e",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:a11y": "jest tests/a11y",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:profiles": "playwright test --grep profile-detection",
    "test:familiar": "playwright test --project=familiar-desktop-chromium",
    "test:jovem": "playwright test --project=jovem-desktop-chromium",
    "test:senior": "playwright test --project=senior-desktop-chromium",
    "test:accessibility": "playwright test --grep a11y",
    "test:api": "vitest run api/__tests__",
    "test:frontend": "vitest run src/__tests__",
    "test:cover-images": "vitest run src/__tests__/blog-cover-images.test.js",
    "test:watch": "vitest --watch",
    "lint:syntax-api": "find api -name '*.js' -exec node -c {} \\; || (echo 'API syntax errors found' && exit 1)",
    "lint:encoding-api": "find api -name '*.js' -exec file {} \\; | grep -v 'UTF-8\\|ASCII' && (echo 'Non-UTF8 API files found' && exit 1) || echo 'All API files are UTF-8 compliant'",
    "validate:api": "npm run lint:syntax-api && npm run lint:encoding-api",
    "validate:blog-images": "bash scripts/verify-blog-images.sh",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "deploy": "sudo ./scripts/nextjs-deploy.sh",
    "deploy:local": "./scripts/nextjs-deploy-local.sh",
    "deploy:legacy": "sudo ./scripts/fixed-deploy.sh",
    "deploy:quick": "sudo ./scripts/quick-deploy.sh",
    "deploy:verify": "./scripts/deploy-verify.sh",
    "deploy:health": "curl -f https://saraivavision.com.br || echo 'Health check failed'",
    "optimize:images": "node scripts/optimize-blog-images.js",
    "verify:blog-images": "node scripts/verify-blog-images.js",
    "test:blog-covers": "node scripts/test-cover-images.js",
    "generate:manifest": "node scripts/generate-image-manifest.js",
    "validate:images": "node scripts/validate-images-ci.js",
    "validate:blog-compliance": "node scripts/validate-blog-compliance-simple.js",
    "audit:blog-images": "node scripts/validate-blog-compliance-simple.js",
    "fix:image-typos": "bash scripts/fix-image-typos.sh",
    "validate:bundle": "node scripts/validate-client-bundle.js",
    "production:check": "npm run build && npm run test:run"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.2.15",
    "@stripe/stripe-js": "^8.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.18",
    "dompurify": "^3.2.7",
    "esbuild": "^0.25.10",
    "fast-xml-parser": "^4.5.0",
    "framer-motion": "^12.23.22",
    "glob": "^10.4.5",
    "hypertune": "^2.10.0",
    "i18next": "^23.11.5",
    "lucide-react": "^0.285.0",
    "marked": "^16.3.0",
    "next": "^15.5.4",
    "posthog-js": "^1.200.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^14.1.3",
    "react-router-dom": "^6.16.0",
    "resend": "^6.1.0",
    "stripe": "^19.0.0",
    "swr": "^2.3.6",
    "tailwind-merge": "^1.14.0",
    "web-vitals": "^5.1.0",
    "workbox-build": "^7.3.0",
    "workbox-cacheable-response": "^7.3.0",
    "workbox-core": "^7.3.0",
    "workbox-expiration": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0",
    "zod": "^3.25.76"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@axe-core/react": "^4.10.2",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.36.0",
    "@next/eslint-plugin-next": "^15.5.4",
    "@playwright/test": "^1.55.1",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.6.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^30.2.0",
    "eslint": "^9.36.0",
    "eslint-config-next": "^15.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.4.0",
    "gray-matter": "^4.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-axe": "^9.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^23.0.1",
    "node-mocks-http": "^1.17.2",
    "postcss": "^8.4.31",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sudo-psc/saraivavision-site-v2.git"
  },
  "homepage": "https://saraivavision.com.br",
  "bugs": {
    "url": "https://github.com/Sudo-psc/saraivavision-site-v2/issues"
  },
  "overrides": {
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.5.0",
    "source-map": "^0.7.6",
    "inflight": "npm:lru-cache@^11.0.2"
  }
}
