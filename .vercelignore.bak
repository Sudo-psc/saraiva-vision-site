# Exclude node_modules and dependencies
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Exclude development and build files
.cache/
.temp/
build/

# Exclude large media files from public directory
public/**/*.mp3
public/**/*.wav
public/**/*.m4a
public/img/**/*
public/Icones/**/*
public/images/**/*
public/Podcasts/**/*
public/**/*.pdf
public/**/*.zip
public/**/*.tar.gz

# Exclude environment files
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Exclude IDE files
.vscode/
.idea/
*.swp
*.swo

# Exclude OS files
.DS_Store
Thumbs.db

# Exclude test coverage
coverage/
.nyc_output/

# Exclude source maps
*.map

# Only include the built assets from dist directory
!dist/assets/
!dist/index.html
!dist/robots.txt
!dist/sitemap.xml
!dist/site.webmanifest
!dist/favicon.ico
!dist/**/*.ico
!dist/**/*.png
!dist/**/*.jpg
!dist/**/*.svg
!dist/**/*.webp