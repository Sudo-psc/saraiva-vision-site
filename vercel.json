{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "functions": {
    "api/**": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "regions": [
    "gru1",
    "iad1"
  ],
  "functionFailoverRegions": [
    "iad1",
    "cle1"
  ]
}