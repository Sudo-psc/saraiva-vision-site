{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "functions": {
    "api/contact/index.js": {
      "maxDuration": 10,
      "memory": 256
    },
    "api/contact/**": {
      "maxDuration": 10,
      "memory": 256
    },
    "api/appointments/**": {
      "maxDuration": 15,
      "memory": 512
    },
    "api/appointments/reminders.js": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/podcast/**": {
      "maxDuration": 30,
      "memory": 512
    },
    "api/podcast/sync.js": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/chatbot.js": {
      "maxDuration": 20,
      "memory": 512
    },
    "api/dashboard/**": {
      "maxDuration": 10,
      "memory": 256
    },
    "api/outbox/**": {
      "maxDuration": 30,
      "memory": 512
    },
    "api/outbox/drain.js": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/webhooks/**": {
      "maxDuration": 10,
      "memory": 256
    },
    "api/monitoring/**": {
      "maxDuration": 15,
      "memory": 512
    },
    "api/health.js": {
      "maxDuration": 5,
      "memory": 128
    },
    "api/ping.js": {
      "maxDuration": 5,
      "memory": 128
    },
    "api/**": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://saraivavision.com.br"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        },
        {
          "key": "Access-Control-Max-Age",
          "value": "86400"
        }
      ]
    },
    {
      "source": "/img/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/Podcasts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/about",
      "destination": "/"
    },
    {
      "source": "/services/:path*",
      "destination": "/"
    },
    {
      "source": "/servicos/:path*",
      "destination": "/"
    },
    {
      "source": "/blog/:path*",
      "destination": "/"
    },
    {
      "source": "/contact",
      "destination": "/"
    },
    {
      "source": "/contato",
      "destination": "/"
    },
    {
      "source": "/testimonials",
      "destination": "/"
    },
    {
      "source": "/depoimentos",
      "destination": "/"
    },
    {
      "source": "/faq",
      "destination": "/"
    },
    {
      "source": "/admin/:path*",
      "destination": "/"
    },
    {
      "source": "/dashboard/:path*",
      "destination": "/"
    },
    {
      "source": "/privacy",
      "destination": "/"
    },
    {
      "source": "/privacidade",
      "destination": "/"
    },
    {
      "source": "/podcast",
      "destination": "/"
    }
  ],
  "regions": [
    "gru1"
  ],
  "crons": [
    {
      "path": "/api/outbox/drain",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/podcast/sync",
      "schedule": "0,30 * * * *"
    },
    {
      "path": "/api/appointments/reminders",
      "schedule": "0 */2 * * *"
    },
    {
      "path": "/api/monitoring/health",
      "schedule": "*/10 * * * *"
    }
  ]
}