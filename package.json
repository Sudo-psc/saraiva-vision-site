    "lucide-react": "^0.285.0",
    "motion": "^12.23.16",
    "music-metadata": "^10.9.1",
    "posthog-js": "^1.266.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^14.1.2",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.16.0",
    "resend": "^6.0.2",
    "server-only": "^0.0.1",
    "sharp": "^0.33.5",
