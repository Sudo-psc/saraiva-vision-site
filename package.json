    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.16",
    "jsdom": "^23.0.1",
    "node-mocks-http": "^1.17.2",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vitest": "^1.0.4"
  },
  "repository": {
    "type": "git",
